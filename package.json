{
  "name": "eslint-plugin-kdu",
  "version": "5.0.0",
  "description": "Official ESLint plugin for Kdu.js",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint . --rulesdir eslint-internal-rules",
    "preversion": "npm run update && git add .",
    "version": "npm run lint -- --fix && git add .",
    "update": "node ./tools/update.js"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/kdujs/eslint-plugin-kdu#readme",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslint-config",
    "kdu",
    "kdujs",
    "rules"
  ],
  "author": "NKDuy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kdujs/eslint-plugin-kdu.git"
  },
  "bugs": {
    "url": "https://github.com/kdujs/eslint-plugin-kdu/issues"
  },
  "engines": {
    "node": ">=6.5"
  },
  "peerDependencies": {
    "eslint": "^5.0.0"
  },
  "dependencies": {
    "kdu-eslint-parser": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^4.2.16",
    "babel-eslint": "^8.2.2",
    "chai": "^4.1.0",
    "eslint": "^5.2.0",
    "eslint-plugin-eslint-plugin": "^1.4.0",
    "eslint-plugin-kdu-libs": "^3.0.2",
    "eslint4b": "^5.1.0",
    "lodash": "^4.17.4",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "typescript": "^3.1.3",
    "typescript-eslint-parser": "^20.0.0"
  }
}
